<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <!-- <include layout="@layout/common_head_1"/> -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="@dimen/dimen_20_dip"
        android:paddingRight="@dimen/dimen_20_dip" >

        <RelativeLayout
            android:id="@+id/rl_mine_set_about"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_70_dip" >

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="@dimen/dimen_20_dip"
                android:layout_height="@dimen/dimen_20_dip"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_about" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_15_dip"
                android:layout_toRightOf="@+id/imageView3"
                android:text="关于e鲜"
                android:textColor="#323232"
                android:textSize="@dimen/dimen_10_sp" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="@dimen/dimen_24_dip"
                android:layout_height="@dimen/dimen_24_dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:padding="@dimen/dimen_2_dip"
                android:src="@drawable/right_black_arrows" />

            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1_dip"
                android:layout_alignParentBottom="true"
                android:background="#F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_mine_set_feedback"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_70_dip" >

            <ImageView
                android:id="@+id/imageView3_1"
                android:layout_width="@dimen/dimen_20_dip"
                android:layout_height="@dimen/dimen_20_dip"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_feedback" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_15_dip"
                android:layout_toRightOf="@+id/imageView3_1"
                android:text="意见反馈"
                android:textColor="#323232"
                android:textSize="@dimen/dimen_10_sp" />

            <ImageView
                android:layout_width="@dimen/dimen_24_dip"
                android:layout_height="@dimen/dimen_24_dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:padding="@dimen/dimen_2_dip"
                android:src="@drawable/right_black_arrows" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1_dip"
                android:layout_alignParentBottom="true"
                android:background="#F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_mine_set_servicer"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_70_dip" >

            <ImageView
                android:id="@+id/imageView3_2"
                android:layout_width="@dimen/dimen_20_dip"
                android:layout_height="@dimen/dimen_20_dip"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_servicer" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_15_dip"
                android:layout_toRightOf="@+id/imageView3_2"
                android:text="联系客服"
                android:textColor="#323232"
                android:textSize="@dimen/dimen_10_sp" />

            <ImageView
                android:layout_width="@dimen/dimen_24_dip"
                android:layout_height="@dimen/dimen_24_dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:padding="@dimen/dimen_2_dip"
                android:src="@drawable/right_black_arrows" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1_dip"
                android:layout_alignParentBottom="true"
                android:background="#F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_mine_set_twodimensioncode"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_70_dip"
            android:visibility="gone" >

            <ImageView
                android:id="@+id/imageView3_3"
                android:layout_width="@dimen/dimen_20_dip"
                android:layout_height="@dimen/dimen_20_dip"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_two_dimension_code" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_15_dip"
                android:layout_toRightOf="@+id/imageView3_3"
                android:text="生成二维码"
                android:textColor="#323232"
                android:textSize="@dimen/dimen_10_sp" />

            <ImageView
                android:layout_width="@dimen/dimen_24_dip"
                android:layout_height="@dimen/dimen_24_dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:padding="@dimen/dimen_2_dip"
                android:src="@drawable/right_black_arrows" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1_dip"
                android:layout_alignParentBottom="true"
                android:background="#F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_mine_set_alter_loginpassword"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_70_dip" >

            <ImageView
                android:id="@+id/imageView3_4"
                android:layout_width="@dimen/dimen_20_dip"
                android:layout_height="@dimen/dimen_20_dip"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_password" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_15_dip"
                android:layout_toRightOf="@+id/imageView3_4"
                android:text="修改登录密码"
                android:textColor="#323232"
                android:textSize="@dimen/dimen_10_sp" />

            <ImageView
                android:layout_width="@dimen/dimen_24_dip"
                android:layout_height="@dimen/dimen_24_dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:padding="@dimen/dimen_2_dip"
                android:src="@drawable/right_black_arrows" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1_dip"
                android:layout_alignParentBottom="true"
                android:background="#F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_mine_set_alter_paypassword"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_70_dip" >

            <ImageView
                android:id="@+id/imageView3_5"
                android:layout_width="@dimen/dimen_20_dip"
                android:layout_height="@dimen/dimen_20_dip"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_password" />

            <TextView
                android:id="@+id/tv_alter_paypwd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_15_dip"
                android:layout_toRightOf="@+id/imageView3_5"
                android:text="修改支付密码"
                android:textColor="#323232"
                android:textSize="@dimen/dimen_10_sp" />

            <ImageView
                android:layout_width="@dimen/dimen_24_dip"
                android:layout_height="@dimen/dimen_24_dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:padding="@dimen/dimen_2_dip"
                android:src="@drawable/right_black_arrows" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1_dip"
                android:layout_alignParentBottom="true"
                android:background="#F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_mine_check_update"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_70_dip"
            android:visibility="visible" >

            <ImageView
                android:id="@+id/imageView3_8"
                android:layout_width="@dimen/dimen_20_dip"
                android:layout_height="@dimen/dimen_20_dip"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_check_update" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_15_dip"
                android:layout_toRightOf="@+id/imageView3_8"
                android:text="检查更新"
                android:textColor="#323232"
                android:textSize="@dimen/dimen_10_sp" />

            <TextView
                android:id="@+id/tv_version_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:textColor="@color/graytext"
                android:textSize="@dimen/dimen_10_sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1_dip"
                android:layout_alignParentBottom="true"
                android:background="#F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_mine_set_clean_cache"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_70_dip" >

            <ImageView
                android:id="@+id/imageView3_7"
                android:layout_width="@dimen/dimen_20_dip"
                android:layout_height="@dimen/dimen_20_dip"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_clean_cache" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_15_dip"
                android:layout_toRightOf="@+id/imageView3_7"
                android:text="清除缓存"
                android:textColor="#323232"
                android:textSize="@dimen/dimen_10_sp" />

            <TextView
                android:id="@+id/tv_clean_cache"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:textColor="@color/graytext"
                android:textSize="@dimen/dimen_10_sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1_dip"
                android:layout_alignParentBottom="true"
                android:background="#F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_mine_set_exitlogin"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_70_dip" >

            <ImageView
                android:id="@+id/imageView3_6"
                android:layout_width="@dimen/dimen_20_dip"
                android:layout_height="@dimen/dimen_20_dip"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_exit_login" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_15_dip"
                android:layout_toRightOf="@+id/imageView3_6"
                android:text="退出当前账户"
                android:textColor="#323232"
                android:textSize="@dimen/dimen_10_sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1_dip"
                android:layout_alignParentBottom="true"
                android:background="#F0F0F0" />
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>