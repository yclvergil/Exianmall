<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dimen_60_dip"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/iv_common_head_2_return"
        android:layout_width="@dimen/dimen_48_dip"
        android:layout_height="@dimen/dimen_28_dip"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/dimen_10_dip"
        android:src="@drawable/ic_return" />

    <TextView
        android:id="@+id/tv_common_head_2_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="注册（2/2）"
        android:textColor="@color/white"
        android:textSize="@dimen/dimen_11_sp" />

</RelativeLayout>